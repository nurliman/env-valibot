{
  "name": "@nurliman/env-valibot",
  "version": "0.7.2",
  "description": "",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Nurliman Diara",
    "email": "nurlimandiara@gmail.com",
    "url": "https://nurliman.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nurliman/env-valibot",
    "directory": "packages/env-valibot"
  },
  "keywords": [],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "require": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./nextjs": {
      "types": "./dist/nextjs.d.ts",
      "import": "./dist/nextjs.js",
      "require": "./dist/nextjs.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./presets": {
      "types": "./dist/presets.d.ts",
      "import": "./dist/presets.js",
      "require": "./dist/presets.cjs"
    }
  },
  "files": [
    "dist",
    "package.json",
    "*.md"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prepack": "cpy '../../README.md' '../../LICENSE' .",
    "postinstall": "bun run build"
  },
  "devDependencies": {
    "@valibot/valibot": "npm:@jsr/valibot__valibot@^0.42.1",
    "cpy-cli": "^3",
    "expect-type": "^1.1.0",
    "tsup": "^8.3.5"
  },
  "peerDependencies": {
    "@valibot/valibot": ">=0.31.0 <0.32.0 || >=0.33.2",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
